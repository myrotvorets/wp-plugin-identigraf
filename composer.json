{
	"name": "myrotvorets/wp-identigraf",
	"type": "wordpress-plugin",
	"description": "IDentigraF integration for WordPress",
	"keywords": [
	],
	"license": "MIT",
	"autoload": {
		"classmap": [
			"inc/"
		]
	},
	"require": {
		"php": "^7.4.0 || ^8.0.0",
		"composer/installers": "^2.0",
		"firebase/php-jwt": "^6.0",
		"wildwolf/singleton-trait": "^1.0.1",
		"wildwolf/wp-request-context": "^1.0"
	},
	"require-dev": {
		"automattic/vipwpcs": "^3.0",
		"johnpbloch/wordpress-core": "^6.3.2",
		"php-stubs/wordpress-stubs": "^6.3.2",
		"phpcompatibility/phpcompatibility-wp": "^2.1.4",
		"vimeo/psalm": "^5.15.0",
		"wp-cli/i18n-command": "^2.4.4"
	},
	"config": {
		"sort-packages": true,
		"platform": {
			"php": "7.4"
		},
		"allow-plugins": {
			"composer/package-versions-deprecated": true,
			"composer/installers": true,
			"dealerdirect/phpcodesniffer-composer-installer": true
		}
	},
	"scripts": {
		"phpcs": "phpcs",
		"phpcs:fix": "phpcbf",
		"psalm": "psalm --long-progress --no-cache"
	}
}
